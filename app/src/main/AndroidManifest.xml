<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="umeng.testasynchronous">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ThreadActivity">
        </activity>
        <activity android:name=".HandlerThreadActivity">
        </activity>
        <activity android:name=".IntentServiceActivity">
        </activity>
        <activity android:name=".AsyncTaskActivity">
        </activity>
        <activity android:name=".ExecutorFrameworkActivity">
        </activity>
        <service android:name=".MyIntentService">
        </service>

    </application>
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
</manifest>